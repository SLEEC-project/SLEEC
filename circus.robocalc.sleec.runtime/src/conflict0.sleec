// rule conflict experiment

def_start
	event E0
	event E1
	event E2
	event E3
	measure m0 : boolean
	measure m1 : boolean
	measure m2 : numeric
def_end

rule_start

	// raise warning saying that E1 will always happen after E0
	// suggest combining R0 and R1 to look like R2
	R0 when E0 and m0 then E1
	R1 when E0 and not m0 then E1
	R2 when E0 then E1
	
	
rule_end