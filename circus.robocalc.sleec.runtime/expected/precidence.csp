include "ticktock.csp"

Timed(et) {
	
channel E0
channel E1

-- Rule1 when E0 and ((((0) < (1)) and ((0) != (0))) or (not ((1) == (1)))) then E1

Rule1 = TriggerRule1; MonitoringRule1; Rule1

TriggerRule1 = let
	MTrigger = if ((((0) < (1)) and ((0) != (0))) or (not ((1) == (1)))) then SKIP else TriggerRule1
within E0 -> MTrigger

MonitoringRule1 = E1 -> SKIP

}
